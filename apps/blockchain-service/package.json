    "@types/node": "^20.11.24",
    "@types/passport-jwt": "^3.0.11",
    "@types/supertest": "^2.0.15",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
