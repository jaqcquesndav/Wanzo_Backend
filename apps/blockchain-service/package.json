  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/main.js",
    "dev": "node dist/main.js",
    "test": "jest -c jest.config.ts --runInBand",
    "test:unit": "jest -c jest.config.ts --runInBand --testPathPattern=blockchain.service.spec.ts",
    "test:integration": "jest -c jest.config.ts --runInBand --testPathPattern=(controller|e2e)",
    "test:ipfs:real": "node scripts/test-runner.js ipfs-real",
    "test:ipfs:real:report": "node scripts/test-runner.js ipfs-real:report",
    "test:fabric:report": "node scripts/test-runner.js fabric:report",
    "build:docker": "npm run build"
  },
  "dependencies": {
    "@kiota-suit/blockchain-service": "file:",
    "@nestjs/common": "^10.3.3",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.3",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.3",
    "@nestjs/swagger": "^7.3.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "helmet": "^7.1.0",
    "jwks-rsa": "^3.1.0",
    "nest-winston": "^1.9.4",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
