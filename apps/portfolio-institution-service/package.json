{
    "name":  "@kiota-suit/portfolio-institution-service",
    "version":  "1.0.0",
    "private":  true,
    "scripts":  {
                    "build":  "nest build",
                    "format":  "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
                    "start":  "node dist/apps/portfolio-institution-service/src/main.js",
                    "start:dev":  "nest start --watch",
                    "start:debug":  "nest start --debug --watch",
                    "start:prod":  "node dist/main",
                    "dev":  "nest build \u0026\u0026 node dist/apps/portfolio-institution-service/src/main.js",
                    "debug":  "nest start --debug --watch",
                    "lint":  "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
                    "test":  "jest --config jest.config.js",
                    "test:watch":  "jest --config jest.config.js --watch",
                    "test:cov":  "jest --config jest.config.js --coverage",
                    "test:debug":  "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
                    "test:unit":  "jest --config jest.config.js --selectProjects unit",
                    "test:integration":  "jest --config jest.config.js --selectProjects integration",
                    "test:e2e":  "jest --config jest.config.js test/integration/e2e",
                    "test:all":  "npm run test:unit && npm run test:integration && npm run test:e2e",
                    "test:ci":  "jest --config jest.config.js --coverage --watchAll=false --passWithNoTests",
                    "build:docker":  "nest build --config ./tsconfig.build.json --webpack --webpackConfigPath webpack-hmr.config.js"
                },
    "dependencies":  {
                         "@nestjs/axios":  "^3.0.2",
                         "@nestjs/common":  "^10.3.3",
                         "@nestjs/config":  "^3.2.0",
                         "@nestjs/core":  "^10.3.3",
                         "@nestjs/jwt":  "^10.2.0",
                         "@nestjs/microservices":  "^10.3.0",
                         "@nestjs/passport":  "^10.0.3",
                         "@nestjs/platform-express":  "^10.3.3",
                         "@nestjs/schedule":  "^4.0.0",
                         "@nestjs/swagger":  "^7.3.0",
                         "@nestjs/terminus":  "^10.2.3",
                         "@nestjs/typeorm":  "^10.0.2",
                         "axios":  "^1.6.7",
                         "bcryptjs":  "^2.4.3",
                         "class-transformer":  "^0.5.1",
                         "class-validator":  "^0.14.1",
                         "express-rate-limit":  "^7.1.5",
                         "helmet":  "^7.1.0",
                         "joi":  "^17.12.2",
                         "kafkajs":  "^2.2.4",
                         "nest-winston":  "^1.9.4",
                         "passport":  "^0.7.0",
                         "passport-jwt":  "^4.0.1",
                         "pg":  "^8.11.3",
                         "prom-client":  "^15.1.0",
                         "reflect-metadata":  "^0.2.1",
                         "rxjs":  "^7.8.1",
                         "swagger-ui-express":  "^5.0.0",
                         "typeorm":  "^0.3.20",
                         "winston":  "^3.11.0"
                     },
    "devDependencies":  {
                            "@nestjs/cli":  "^10.3.2",
                            "@nestjs/schematics":  "^10.1.1",
                            "@nestjs/testing":  "^10.3.3",
                            "@types/bcryptjs":  "^2.4.6",
                            "@types/express":  "^4.17.21",
                            "@types/jest":  "^29.5.12",
                            "@types/node":  "^20.11.24",
                            "@types/passport-jwt":  "^4.0.1",
                            "@types/supertest":  "^6.0.2",
                            "jest":  "^29.7.0",
                            "supertest":  "^6.3.4",
                            "ts-jest":  "^29.1.2",
                            "ts-loader":  "^9.5.2",
                            "ts-node":  "^10.9.2",
                            "tsconfig-paths":  "^4.2.0",
                            "typescript":  "^5.3.3",
                            "webpack":  "^5.99.9",
                            "webpack-node-externals":  "^3.0.0"
                        }
}
