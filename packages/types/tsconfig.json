{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Wanzobe Types Package",
  "compilerOptions": {
    "declaration": true,
    "declarationMap": false,
    "emitDeclarationOnly": false,
    "outDir": "./dist",
    "rootDir": "./",
    "composite": true,
    "skipLibCheck": true,
    "types": [],
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node"
  },
  "include": [
    "*.d.ts",
    "**/*.d.ts"
  ],
  "exclude": ["node_modules", "dist"]
}
